    <link rel="stylesheet" href="css/appv2.10.css" />
    <link rel="stylesheet" href="css/mail/mail.css" />
    <link rel="stylesheet" href="css/dragdrop.css" />
    <link rel="stylesheet" href="css/quill.snow.css" />
    <link rel="stylesheet" href="css/quill.bubble.css" />
    <link rel="stylesheet" href="OnetezSoft.styles.css">
    <link rel="stylesheet" href="_content/BlazorDateRangePicker/daterangepicker.min.css" />
    <link href="_content/Blazor.ContextMenu/blazorContextMenu.min.css" rel="stylesheet" />
    <component type="typeof(HeadOutlet)" render-mode="ServerPrerendered" />
</head>

<body>
    <component type="typeof(App)" render-mode="ServerPrerendered" />

    <div id="blazor-error-ui">
        <environment include="Staging,Production">
            An error has occurred. This application may no longer respond until reloaded.
        </environment>
        <environment include="Development">
            An unhandled exception has occurred. See browser dev tools for details.
        </environment>
        <a href="" class="reload">Reload</a>
        <a class="dismiss">🗙</a>
    </div>

    <script src="~/js/app.js"></script>
    <script src="~/js/chart.min.js"></script>
    <script src="~/js/quill.js"></script>
    <script src="~/js/mention.js"></script>
    <script src="~/js/bingmap.js"></script>
    <script src="_framework/blazor.server.js" autostart="false"></script>
    <script src="~/js/boot.js"></script>
    <script src="~/js/myjs.js"></script>
    <!-- Bao gồm thư viện jQuery và Notify -->
    <script src="https://code.jquery.com/jquery-3.6.0.min.js"></script>
    <script src="https://cdnjs.cloudflare.com/ajax/libs/notify/0.4.2/notify.min.js"></script>
    <script src="_content/BlazorDateRangePicker/clickAndPositionHandler.js"></script>
    <script src="_content/Blazored.TextEditor/quill-blot-formatter.min.js"></script>
    <script src="_content/Blazored.TextEditor/Blazored-BlazorQuill.js"></script>
